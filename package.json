        "@mui/material": "^5.17.1",
        "@tanstack/react-query": "^5.74.4",
        "@tanstack/react-router": "^1.116.0",
        "@types/js-cookie": "^3.0.6",
        "async-mutex": "^0.5.0",
        "js-cookie": "^3.0.5",
        "lodash": "^4.17.21",
        "neverthrow": "^8.2.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-use-websocket": "^4.13.0",
        "tailwind-merge": "^2.6.0",
        "zod": "^3.24.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
