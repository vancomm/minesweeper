        "eslint-plugin-react-refresh": "^0.4.20",
        "gh-pages": "^6.3.0",
        "globals": "^15.15.0",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.0",
        "vite": "^5.4.18",
